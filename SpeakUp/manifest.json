{
	"manifest_version": 2,
	"name": "SpeakUp",
	"version": "1.0",
	"description": "Speaks the selected text , as hearing is more powerful than reading",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},
	"permissions": ["tts", "contextMenus"]
}
